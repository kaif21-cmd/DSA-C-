#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cout<<"Enter the size of the array\n";
    cin>>n;
    int ar[n];
    cout<<"Enter the element in the array one by one\n";
    for(int i=0;i<n;i++)
    cin>>ar[i];
    int result=0;
    for(int i=0;i<n;i++)
    {
        result=result^ar[i];
    }
    cout<<"ODD OCURRENCE NO IS:\n";
    cout<<result<<endl;
    return 0;
}
